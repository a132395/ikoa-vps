[default]
#使用的网盘，gd or od
usedrive = gd
#下载文件夹，以/开头
destination_path = /ikoadownloads
#日志，以/开头
log_path = /ikoalogs
#用户名
adminuser = admin
#密码
adminpassword = passwd
#合并
merge_bool = true
#序列码
serial_code = good
#月额
monthly_only = true
#输出文件，cid or num
output_filename = cid
#首次自动生成，删除下次启动即可再次生成
secret_key = 
#运行的端口
runport = 5000
#一次执行的个数(比如一次添加10个，down_time = 5 代表下载五个再上传五个，以此类推)
down_time = 5

[rclone_conf_gd]
#gd的folder id
team_drive_id = 123




